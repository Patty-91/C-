#include "pch.h" // This is a Standard Header File, it will help our solution to integrate all of our files
#include <iostream> // This is the Basic input/output library, like the <stdio.h> on C
#include <vector>
using namespace std;


int main() {
//2D Vectors

vector <vector<int>> my2DVector{
// RC
{00,01,02},
{10,11,12},
{20,21,22}
// {Row 0 Col 0, Row 0 Col 1, Row 0 Col 2}
// {Row 1 Col 0, Row 1 Col 1, Row 1 Col 2}
// {Row 2 Col 0, Row 2 Col 1, Row 2 Col 2}
};


cout << "Row 0 Col 0 " << my2DVector.at(0).at(0) << endl;
cout << "Row 0 Col 1 " << my2DVector.at(0).at(1) << endl;
cout << "Row 0 Col 2 " << my2DVector.at(0).at(2) << endl;

cout << "Row 1 Col 0 " << my2DVector.at(1).at(0) << endl;
cout << "Row 1 Col 1 " << my2DVector.at(1).at(1) << endl;
cout << "Row 1 Col 2 " << my2DVector.at(1).at(2) << endl;

cout << "Row 2 Col 0 " << my2DVector.at(2).at(0) << endl;
cout << "Row 2 Col 1 " << my2DVector.at(2).at(1) << endl;
cout << "Row 2 Col 2 " << my2DVector.at(2).at(2) << endl;


system("pause");
}
